<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="findEagle" table="FIND">
      <meta attribute="class-description">
         This class contains location of the eagle. 
      </meta>
      <id name="id" type="int" column="id">
         <generator class="native"/>
      </id>
      <property name="stateCounties" column="state_counties" type="string"/>
   </class>
</hibernate-mapping>

<hibernate-mapping>
   <class name="reportEagle" table="REPORT">
      <meta attribute="class-description">
         This class contains reports of eagle sighting.
      </meta>
      <id name="id" type="int" column="id">
         <generator class="native"/>
      </id>
      <property name="stateCounties" column="state_counties" type="string"/>
   </class>
</hibernate-mapping>
